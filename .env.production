# Deployment Configuration for Trade Manthan React Frontend
# Copy this to .env.production before building

# API Configuration
REACT_APP_API_URL=https://trademanthan.in
REACT_APP_API_TIMEOUT=10000

# Application Configuration
REACT_APP_APP_NAME=Trade Manthan
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_SENTRY=false

# External Services (if needed)
REACT_APP_SENTRY_DSN=
REACT_APP_GOOGLE_ANALYTICS_ID=

# Performance Configuration
REACT_APP_CACHE_DURATION=300000
REACT_APP_MAX_RETRIES=3
REACT_APP_RETRY_DELAY=1000
